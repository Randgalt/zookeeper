<?xml version="1.0" encoding="UTF-8"?>

<!-- doc here: https://spotbugs.readthedocs.io/en/stable/filter.html -->
<FindBugsFilter>
    <!-- this work work on JDK11  https://github.com/spotbugs/spotbugs-maven-plugin/issues/92 -->
    <Bug pattern="RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE"/>

    <!-- this problem is to be addressed in ZOOKEEPER-3227 -->
    <Bug pattern="DM_DEFAULT_ENCODING"/>

    <Match>
        <Or>
            <Class name="org.apache.zookeeper.server.rpc.ACL"/>
            <Class name="org.apache.zookeeper.server.rpc.ACLs"/>
            <Class name="~org\.apache\.zookeeper\.server\.rpc\.ConfigData.*"/>
            <Class name="~org\.apache\.zookeeper\.server\.rpc\.Transaction.*"/>
        </Or>
        <Bug pattern="UUF_UNUSED_FIELD"/>
    </Match>

    <Match>
        <Class name="~org\.apache\.zookeeper\.rpc\.generated.*"/>
    </Match>
</FindBugsFilter>

